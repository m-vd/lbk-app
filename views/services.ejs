<% include ./partials/header.ejs %>

<form>
    <div class = "form-group">
        <input type="text" class="form-control" name="request[name]" placeholder="Nama anda">
    </div>
    <div class = "form-group">
        <input type="text" class="form-control" name="request[nim]" placeholder="Nim anda">
    </div>
    <div class = "form-group">
        <select class="form-control" name="request[type]">
          <option value = "konseling">Konseling</option>
          <option value = "terapi">Terapi</option>
          <option value = "seminar">Seminar</option>
          <option value = "training">Training</option>
          <option></option>
        </select>
    </div>
    <div class = "form-group">
        <input type="datetime-local" class="form-control" name="request[date]" placeholder="Date anda">
    </div>

</form>

<% psychologists.forEach(function (f){ %>
    <% f.schedule.forEach(function (d){ %>
        <h4><%=moment(d.unavailable).format('MMMM Do YYYY')%></h4>
    <% }); %>
<% }); %>

<% include ./partials/footer.ejs %>